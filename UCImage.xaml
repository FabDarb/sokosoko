<UserControl x:Class="sokosoko.UCImage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:sokosoko"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="50">
    <Grid Width="50" Height="50">
        <Image x:Name="wall" Source="/wall.png"></Image>
        <Image x:Name="ground" Source="/ground.png"></Image>
        <Image x:Name="crate" Source="/crate.png"></Image>
        <Image x:Name="crateJu" Source="/crateInJu.png"></Image>
        <Image x:Name="player_bottom" Source="/player_bottom.png"></Image>
        <Image x:Name="player_left" Source="/player_left.png"></Image>
        <Image x:Name="player_right" Source="/player_right.png"></Image>
        <Image x:Name="player_top" Source="/player_top.png"></Image>
        <Image x:Name="player_Case" Source="/player_withCase.png"></Image>
        <Image x:Name="environment" Source="/environment.png"></Image>
        
    </Grid>
</UserControl>
